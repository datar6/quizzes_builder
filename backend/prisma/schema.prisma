generator client {
  provider = "prisma-client-js"
  // output   = "../node_modules/.prisma/client" // 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quiz {
  id        String   @id @default(cuid())
  title     String
  questions Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("quizzes")
}